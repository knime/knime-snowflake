<FindBugsFilter>
    <Match>
        <Class name="~.*\.*Test"/>
        <Or>
            <Field type="org.mockito.junit.MockitoRule"/>
            <Field type="org.junit.rules.ExpectedException"/>
        </Or>
        <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD"/>
    </Match>
</FindBugsFilter>
