            factory="org.knime.database.extension.snowflake.functions.bool.BoolXorDBAggregationFunction$Factory"
            dbTypes="snowflake"/>

<!-- Hash -->
    <!-- HASH_AGG -->

<!-- Semi-structure Data Aggregation -->
