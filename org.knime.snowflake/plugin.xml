            factory="org.knime.database.function.aggregation.impl.functions.RegrR2DBAggregationFunction$Factory"
            dbTypes="snowflake"/>
      <AggregationFunction
            factory="org.knime.database.function.aggregation.impl.functions.RegrSlopeDBAggregationFunction$Factory"
